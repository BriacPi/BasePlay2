# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Map static resources from the /public folder to the /assets URL path
GET  /SuspectRow/:date/:caisse/:groupe/:agence/:pdv/:metric                                 controllers.Application.find(date : String,caisse : String, groupe : String, agence :String,pdv :String,metric :String)
GET  /SuspectRow/:id                                                                        controllers.Application.findWithId(id:Long)
GET  /Edition/:id                                                                           controllers.Application.edit(id:Long)
POST /findAndEdit/:id                                                                       controllers.Application.saveEdition(id:Long)
GET  /DetectedOnly                                                                          controllers.Application.detectedOnly()
GET  /BeingProcessed                                                                        controllers.Application.beingProcessed()
GET  /Solved                                                                                controllers.Application.solved()
GET  /assets/*file                                                                          controllers.Assets.versioned(path="/public", file: Asset)
GET  /data                                                                                  controllers.Application.sendRequestToApi()
GET  /                                                                                      controllers.Application.redirect()