# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Map static resources from the /public folder to the /assets URL path
GET  /SuspectRow/:date/:caisse/:groupe/:agence/:pdv/:metric                                 controllers.Application.find(date : String,caisse : String, groupe : String, agence :String,pdv :String,metric :String)
GET  /SuspectRow/:id                                                                        controllers.Application.findWithId(id:Long)
GET  /Status/:id                                                                            controllers.Application.editStatus(id:Long)
GET  /Type/:id                                                                              controllers.Application.editNature(id:Long)
GET  /Comment/:id                                                                           controllers.Application.editComment(id:Long)
GET  /Admin/:id                                                                             controllers.Application.editAdmin(id:Long)
POST /findAndEditNature/:id                                                                 controllers.Application.saveEditionNature(id:Long)
POST /findAndEditAdmin/:id                                                                 controllers.Application.saveEditionAdmin(id:Long)
POST /findAndEditComment/:id                                                                 controllers.Application.saveEditionComment(id:Long)
POST /findAndEditStatus/:id                                                                 controllers.Application.saveEditionStatus(id:Long)
GET  /DetectedOnly                                                                          controllers.Application.detectedOnly()
GET  /BeingProcessed                                                                        controllers.Application.beingProcessed()
GET  /Solved                                                                                controllers.Application.solved()
GET  /assets/*file                                                                          controllers.Assets.versioned(path="/public", file: Asset)
GET  /data                                                                                  controllers.Application.sendRequestToApi()
GET  /                                                                                      controllers.Application.redirect()