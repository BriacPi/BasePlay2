# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~



# Map static resources from the /public folder to the /assets URL path
GET  /add/:date/:caisse/:groupe/:agence/:pdv/:abnormalMetric                        controllers.Application.add(date : String,caisse : String, groupe : String, agence :String,pdv :String, abnormalMetric :String )
GET  /editStatus/:date/:caisse/:groupe/:agence/:pdv/:metric/:status                         controllers.Application.editStatus(date : String,caisse : String, groupe : String, agence :String,pdv :String ,metric :String,status:String)
GET  /editType/:date/:caisse/:groupe/:agence/:pdv/:metric/:errorType                        controllers.Application.editType(date : String,caisse : String, groupe : String, agence :String,pdv :String ,metric :String,errorType:String)
GET  /editComment/:date/:caisse/:groupe/:agence/:pdv/:metric/:comment                       controllers.Application.editComment(date : String,caisse : String, groupe : String, agence :String,pdv :String ,metric :String,comment:String)
GET  /editAdmin/:date/:caisse/:groupe/:agence/:pdv/:metric/:admin                           controllers.Application.editAdmin(date : String,caisse : String, groupe : String, agence :String,pdv :String ,metric :String,admin:String)
GET  /all                                                                           controllers.Application.all()
GET  /assets/*file                                                                  controllers.Assets.versioned(path="/public", file: Asset)
GET  /data                                                                          controllers.Application.sendRequestToApi()